public class Solution {
    public boolean isPalindrome(int x) {
       int result=0;
       int tar=x;
       if(x<0){
           return false;
       }
       for(;x!=0;x/=10){
           result=result*10+x%10;
       }
       if(tar==result){
           return true;
       }
       return false;
    }
}
