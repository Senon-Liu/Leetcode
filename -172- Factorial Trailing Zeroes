public class Solution {
    public int trailingZeroes(int n) {
        int result = 0;
    while(n > 1)
    {
        n/=5;
        result += n;

    }
    return result;
    }
}



redo:
public class Solution {
    public int trailingZeroes(int n) {
        if(n<5) return 0;
        int count=0;
        while(n>0){
            n/=5;
            count+=n;
        }
        return count;
    }
}
