public class Solution {
    public int singleNumber(int[] A) {
          int result = 0;
          for (int i = 0; i<A.length; i++)
          {
              result ^= A[i];
          }
          return result;
    }
}


redo:
public class Solution {
    public int singleNumber(int[] A) {
        //if(A==null) return 0;
        int tmp=0;
        for(int i=0;i<A.length;i++){
            tmp^=A[i];
        }
        return tmp;
    }
}
